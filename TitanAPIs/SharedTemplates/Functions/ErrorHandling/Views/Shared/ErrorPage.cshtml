@model TitanLogging.ErrorViewModel
@{
    ViewData["Title"] = "Error";
}

<h1 class="text-danger">An error occured while processing your request</h1>
<h3 class="text-danger"><q>@Model.Message</q></h3>
<br />
<div><a class="btn btn-primary" id="back-link">Go Back</a></div>
<br />
<h4 class="text-danger">Please contact an administrator if this is a recurring problem</h4>
<h4 class="text-danger">Reference: <code>@Model.CorrelationId</code></h4>


<script>
    $('#back-link').attr('href', document.referrer);

    $('#back-link').click(function() {
    history.back();
    return false;
    });
</script>